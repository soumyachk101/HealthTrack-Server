# Django Secret Key
SESSION_SECRET=django-insecure-dev-key-change-in-production

# Gmail SMTP Settings for OTP Emails
EMAIL_HOST_USER=your-email@gmail.com
EMAIL_HOST_PASSWORD=your-app-password

# AI API Keys
GOOGLE_API_KEY=your-google-api-key
OPENAI_API_KEY=your-openai-api-key

# Deployment Settings
DEBUG=False
ALLOWED_HOSTS=your-domain.vercel.app,localhost,127.0.0.1

# CORS Settings (Your Netlify frontend URL)
CORS_ALLOWED_ORIGINS=https://your-frontend.netlify.app
CSRF_TRUSTED_ORIGINS=https://your-domain.vercel.app,https://your-frontend.netlify.app

